% od36.mf -- Odia regular 36 pt parameter file
% Copyright 1996 Jeroen Hellingman

mode_setup;

input odcode;
input odbase;

gensize# := 36pt#; 
uh#     := 0.74pt#;             % unit height, (X-height is 25uh)
uw#     := 0.68pt#;             % unit width
rm#     := 6.0uw#;              % space in between characters
rb#     := 1.5uw#;              % extra space to right of standing bars
slant   := 0;                   % slant of characters
calligraphic := true;		% calligraphic effects
rot     := 30;                  % angle of pen
thick#  := 3uh#;  		% longest diameter of pen            
thin#   := 0.4uh#;  		% smallest diameter of pen

font_size               36pt#;
font_identifier :=      "OD36";

input odia;

end.
% end of od36.mf
